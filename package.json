{
  "name": "@jswork/bootstrap4-custom",
  "version": "1.0.0",
  "type": "module",
  "description": "A lightweight, customizable Bootstrap 4 partial import package.",
  "prettier": "@jswork/style-guide",
  "license": "MIT",
  "main": "lib/style.scss",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "dev": "run-p dev:*",
    "dev:sass": "sass lib/index.scss dist/style.css --watch",
    "dev:sync": "browser-sync start --server --files 'src/**/*'",
    "build": "gulp",
    "release": "release-it"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@jswork/style-guide": "^1.0.3",
    "del": "^7.1.0",
    "gulp": "^5.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-size": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "sass": "^1.90.0"
  },
  "dependencies": {
    "bootstrap": "4"
  },
  "keywords": [
    "bootstrap",
    "jquery",
    "custom"
  ]
}
